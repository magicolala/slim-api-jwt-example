{
    "name": "codes50/slim-framework-4-jwt-auth-example",
    "description": "Slim Framework v4 JWT Auth Example",
    "keywords": [
        "slim",
        "framework",
        "jwt",
        "token",
        "auth",
        "example"
    ],
    "homepage": "https://github.com/CodeS50/Slim-Framework-4-JWT-Auth-Example",
    "license": "MIT",
    "authors": [
        {
            "name": "Kadir Yolalan",
            "email": "kadiryolalan@gmail.com",
            "homepage": "https://github.com/CodeS50",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "codes50/validation": "^1.3",
        "firebase/php-jwt": "^5.0",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.1",
        "tuupola/slim-jwt-auth": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
